<!DOCTYPE html>
<html lang="en">

<head>
  <title>Training</title>
  <title></title>
  <%- include('./layout/head'); %>
    <%- include('./layout/header'); %>
</head>

<body>
  <div class="text-center">
    <form action="api/update-dataset" method="post" enctype="multipart/form-data">
      <input type="file" name="dataset" id="" accept=".zip">
      <input type="submit" value="UpLoad">
    </form>
  </div>



</body>
<script>
  fetch('/api/list-file', {mode: 'cors'})
  .then(function(response) {
    return response.text();
  })
  .then(function(text) {
    console.log('Request successful', text);
  })
  .catch(function(error) {
    log('Request failed', error)
  });
</script>
