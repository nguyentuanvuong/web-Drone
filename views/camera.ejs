<!DOCTYPE html>
<html>

<head>
    <title>Connect Camera</title>
    <%- include('./layout/head'); %>
        <%- include('./layout/header'); %>
</head>

<body>
    <div id="root" class="container" style="display: none;">
        <div id="connect" class="form-floating d-flex">
            <input type="number" class="form-control" name="" id="user_id" value="3469747508247772258">
            <!-- <input type="number" class="form-control" name="" id="user_id" > -->
            <label class="form-label" for="">User ID: </label>
            <button id="btn_connect" class="btn btn-primary">Connect</button>
        </div>
        <div class="row">
            <div id="list_camera"  class="col-3">
                <!-- <button onclick="enableTest()">test video</button> -->
            </div>
            <div class="col-9">
                <div id="view-Cam">
                    <canvas id="results" style="display: none;"></canvas>
                </div>
            </div>
            <textArea id="Serial" class="form-control" rows="5" style="resize: none;" disabled></textArea>
        </div>
        <div>
            <div>
                <input type="text" id="sensor" value="26.65 92 0.26 0">
                <button id="btn_predict" class="btn btn-primary">predict</button>
            </div>
        </div>
    </div>
</body>
<script type="text/javascript" src="/socket.io/socket.io.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/tensorflow/3.11.0/tf.min.js"
    integrity="sha512-ufFDHpWUqU90/5cbhnHp5ZWTtyiYNWqBNn/E5/jDEka1BBA4N74U8/bZn3HP1Ny1Ku4hcs+feHv4KTdb6GbBMA=="
    crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="js/camera.js"></script>

<footer>
    <%- include('./layout/footer');%>
</footer>

</html>