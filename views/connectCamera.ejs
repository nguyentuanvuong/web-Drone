<!DOCTYPE html>
<html>
  <%- include('./bootstrap'); %>
  <head>
    <title>Stream Camera</title>
    <%- include('./layout/header'); %>
  </head>
  <body>
    <div style="margin-top: 60px;"></div>
      <form id="config" action="javascript:alert;">
        <select id="videoSource"></select>
        <select id="fps">
          <option value="1">1 fps</option>
          <option value="5">5 fps</option>
          <option value="10">10 fps</option>
          <option value="15">15 fps</option>
          <option value="30">30 fps</option>
        </select>
        <select id="resolution">
          <option value="144">144p</option>
          <option value="240">240p</option>
          <option value="360">360p</option>
          <option value="640">640p</option>
          <option value="720">720p</option>
          <option value="1280">1280p</option>
        </select>
        <input type="submit" value="Connect">
      </form>
      <video id="video" width="48%" controls autoplay></video>
      <canvas id="preview" style="display: none;" ></canvas>
   
     <img id="image" src="" alt="" width="48%">
     <span> <p id="results"></p></span>
   </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
      <script type="text/javascript" src="/socket.io/socket.io.js"></script>
      <script src="js/socket.io/connectcamera.js"></script>
      <script>
      socket.on('log', (msg) => {
        console.log(msg)
        });
      </script>
  </body>
  <footer>
    <%- include('./layout/footer');%>
  </footer>
</html>